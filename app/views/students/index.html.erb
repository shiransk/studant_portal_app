<h1> Student: </h1>
  <table>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Resume</th>
    </tr>
    <% @students.each do |student| %>
    <tr>
      <td><%= student["first_name"] %></td>
      <td><%= student["last_name"] %></td>
      <td><%= student["email"] %></td>
      <td><%= link_to 'Resume',  "resumes/#{student['id']}" %></td>
    </tr>
    <% end %>
  </table>